{
"version": 2,
"builds": [
    {
    "src": "api/*.js",
    "use": "@vercel/node"
    }
],
"routes": [
    {
    "src": "/",
    "status": 301,
    "headers": {
        "Location": "https://github.com/adithyarao3103/Visitor-Counter"
    }
    },
    {
    "src": "/increment",
    "dest": "/api/increment.js"
    },
    {
    "src": "/show",
    "dest": "/api/show.js"
    }
]
}